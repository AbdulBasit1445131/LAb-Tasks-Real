	}
	
	public class ShellnSort {

		
		
		public int Array[]={55,07,2,3,4,9,77,34,64,14,11};
		
		
		
		public void Sort(){
			
	        
			int c;
			
			    int a = Array.length;
			    for (int j = 1; j < a; j++) {
			        int key = Array[j];
			        int i = j-1;
			        while ( (i > -1) && ( Array[i] > key ) ) {
			        	Array [i+1] = Array [i];
			            i--;
			    
			        }
			        Array[i+1] = key;
		  
			    }
			
			   
			    for (c = 0; c <10 ; c++) 
				   {  System.out.print(Array[c]+",");}
				   
		}
		
		
		
		
	}

